<?xml version="1.0" ?>
<transformation>
  <info>
    <name>11_indicateurs_globaux</name>
    <description>Chargement des indicateurs globaux (unpivot + temps)</description>
  </info>
  <connection>
    <name>morocco_tourism</name>
    <server>localhost</server>
    <type>POSTGRESQL</type>
    <access>Native</access>
    <database>morocco_tourism</database>
    <port>5432</port>
    <username>postgres</username>
    <password>your_password_here</password>
  </connection>
  <step>
    <name>Lire_indicateurs</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>100</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>TextFileInput</type>
    <separator>,</separator>
    <enclosure>&quot;</enclosure>
    <header>Y</header>
    <encoding>UTF-8</encoding>
    <file>
      <name>data/raw/11_indicateurs_globaux.csv</name>
    </file>
    <fields>
      <field>
        <name>Indicateur</name>
        <type>String</type>
      </field>
      <field>
        <name>Annee_2019</name>
        <type>String</type>
      </field>
      <field>
        <name>Annee_2021</name>
        <type>String</type>
      </field>
      <field>
        <name>Annee_2022</name>
        <type>String</type>
      </field>
    </fields>
  </step>
  <step>
    <name>Standardiser_colonnes</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>330</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>SelectValues</type>
    <fields>
      <field>
        <name>Indicateur</name>
        <rename>indicateur</rename>
      </field>
      <field>
        <name>Annee_2019</name>
        <rename>annee_2019</rename>
      </field>
      <field>
        <name>Annee_2021</name>
        <rename>annee_2021</rename>
      </field>
      <field>
        <name>Annee_2022</name>
        <rename>annee_2022</rename>
      </field>
    </fields>
  </step>
  <step>
    <name>Unpivot_annees</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>560</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>RowNormaliser</type>
    <fields>
      <field>
        <typefield>annee_2019</typefield>
        <valuefield>valeur</valuefield>
        <newfield>annee_str</newfield>
      </field>
      <field>
        <typefield>annee_2021</typefield>
        <valuefield>valeur</valuefield>
        <newfield>annee_str</newfield>
      </field>
      <field>
        <typefield>annee_2022</typefield>
        <valuefield>valeur</valuefield>
        <newfield>annee_str</newfield>
      </field>
    </fields>
  </step>
  <step>
    <name>Extraire_annee</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>790</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>ModifiedJavaScriptValue</type>
    <jsScripts>var annee = parseInt(annee_str.replace(/\D/g, ''));</jsScripts>
    <fields>
      <field>
        <name>annee</name>
        <type>Integer</type>
      </field>
    </fields>
  </step>
  <step>
    <name>Nettoyer_valeur</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1020</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>ReplaceInString</type>
    <fields>
      <field>
        <in_stream_name>valeur</in_stream_name>
        <replace_string> </replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>valeur</in_stream_name>
        <replace_string>,</replace_string>
        <replace_by_string/>
      </field>
    </fields>
  </step>
  <step>
    <name>Convertir_types</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1250</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>SelectValues</type>
    <fields>
      <field>
        <name>annee</name>
        <type>Integer</type>
        <format>####</format>
        <length>4</length>
        <precision>0</precision>
      </field>
      <field>
        <name>valeur</name>
        <type>Number</type>
        <format>#.##</format>
        <length>15</length>
        <precision>2</precision>
      </field>
      <field>
        <name>indicateur</name>
        <type>String</type>
        <format/>
        <length>100</length>
        <precision>-1</precision>
      </field>
    </fields>
  </step>
  <step>
    <name>Lookup_temps</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1480</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>DatabaseLookup</type>
    <connection>morocco_tourism</connection>
    <table>dim_temps</table>
    <keys>
      <key>
        <condition>=</condition>
        <name>annee</name>
        <field>annee</field>
      </key>
    </keys>
    <values>
      <value>
        <name>temps_id</name>
        <rename>temps_id</rename>
      </value>
    </values>
  </step>
  <step>
    <name>Inserer_fact_indicateurs</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1710</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>InsertUpdate</type>
    <connection>morocco_tourism</connection>
    <table>fact_indicateurs_globaux</table>
    <commit>1000</commit>
    <use_batch>Y</use_batch>
    <lookup>
      <key>
        <name>temps_id</name>
        <field>temps_id</field>
        <condition>=</condition>
      </key>
      <key>
        <name>nom_indicateur</name>
        <field>indicateur</field>
        <condition>=</condition>
      </key>
    </lookup>
    <update>
      <update>
        <name>valeur</name>
        <field>valeur</field>
        <update>Y</update>
      </update>
    </update>
  </step>
  <hop>
    <from>Lire_indicateurs</from>
    <to>Standardiser_colonnes</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Standardiser_colonnes</from>
    <to>Unpivot_annees</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Unpivot_annees</from>
    <to>Extraire_annee</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Extraire_annee</from>
    <to>Nettoyer_valeur</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Nettoyer_valeur</from>
    <to>Convertir_types</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Convertir_types</from>
    <to>Lookup_temps</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Lookup_temps</from>
    <to>Inserer_fact_indicateurs</to>
    <enabled>Y</enabled>
  </hop>
</transformation>
