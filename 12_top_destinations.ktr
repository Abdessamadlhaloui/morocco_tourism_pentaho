<?xml version="1.0" ?>
<transformation>
  <info>
    <name>12_top_destinations</name>
    <description>Chargement snapshot top destinations (no time dimension)</description>
  </info>
  <connection>
    <name>morocco_tourism</name>
    <server>localhost</server>
    <type>POSTGRESQL</type>
    <access>Native</access>
    <database>morocco_tourism</database>
    <port>5432</port>
    <username>postgres</username>
    <password>your_password_here</password>
  </connection>
  <step>
    <name>Lire_top_destinations</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>100</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>TextFileInput</type>
    <separator>,</separator>
    <enclosure>&quot;</enclosure>
    <header>Y</header>
    <encoding>UTF-8</encoding>
    <file>
      <name>data/raw/12_top_destinations.csv</name>
    </file>
    <fields>
      <field>
        <name>Destination</name>
        <type>String</type>
      </field>
      <field>
        <name>Non_Residents</name>
        <type>String</type>
      </field>
      <field>
        <name>Residents</name>
        <type>String</type>
      </field>
      <field>
        <name>Total</name>
        <type>String</type>
      </field>
      <field>
        <name>Taux_Occupation_pct</name>
        <type>String</type>
      </field>
    </fields>
  </step>
  <step>
    <name>Standardiser_colonnes</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>330</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>SelectValues</type>
    <fields>
      <field>
        <name>Destination</name>
        <rename>destination</rename>
      </field>
      <field>
        <name>Non_Residents</name>
        <rename>non_residents</rename>
      </field>
      <field>
        <name>Residents</name>
        <rename>residents</rename>
      </field>
      <field>
        <name>Total</name>
        <rename>total</rename>
      </field>
      <field>
        <name>Taux_Occupation_pct</name>
        <rename>taux_occupation_pct</rename>
      </field>
    </fields>
  </step>
  <step>
    <name>Nettoyer_numeriques</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>560</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>ReplaceInString</type>
    <fields>
      <field>
        <in_stream_name>non_residents</in_stream_name>
        <replace_string> </replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>non_residents</in_stream_name>
        <replace_string>,</replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>residents</in_stream_name>
        <replace_string> </replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>residents</in_stream_name>
        <replace_string>,</replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>total</in_stream_name>
        <replace_string> </replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>total</in_stream_name>
        <replace_string>,</replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>taux_occupation_pct</in_stream_name>
        <replace_string> </replace_string>
        <replace_by_string/>
      </field>
      <field>
        <in_stream_name>taux_occupation_pct</in_stream_name>
        <replace_string>,</replace_string>
        <replace_by_string/>
      </field>
    </fields>
  </step>
  <step>
    <name>Convertir_types</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>790</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>SelectValues</type>
    <fields>
      <field>
        <name>non_residents</name>
        <type>Integer</type>
        <format>########</format>
        <length>12</length>
        <precision>0</precision>
      </field>
      <field>
        <name>residents</name>
        <type>Integer</type>
        <format>########</format>
        <length>12</length>
        <precision>0</precision>
      </field>
      <field>
        <name>total</name>
        <type>Integer</type>
        <format>########</format>
        <length>12</length>
        <precision>0</precision>
      </field>
      <field>
        <name>taux_occupation_pct</name>
        <type>Number</type>
        <format>#.##</format>
        <length>6</length>
        <precision>2</precision>
      </field>
      <field>
        <name>destination</name>
        <type>String</type>
        <format/>
        <length>100</length>
        <precision>-1</precision>
      </field>
    </fields>
  </step>
  <step>
    <name>Trim_destination</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1020</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>StringOperations</type>
    <fields>
      <field>
        <name>destination</name>
        <trim_type>both</trim_type>
      </field>
    </fields>
  </step>
  <step>
    <name>Inserer_dim_destinations</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1250</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>InsertUpdate</type>
    <connection>morocco_tourism</connection>
    <table>dim_destinations</table>
    <commit>1000</commit>
    <use_batch>Y</use_batch>
    <lookup>
      <key>
        <name>nom_destination</name>
        <field>destination</field>
        <condition>=</condition>
      </key>
    </lookup>
    <update>
      <update>
        <name>nom_destination</name>
        <field>destination</field>
        <update>Y</update>
      </update>
    </update>
  </step>
  <step>
    <name>Lookup_destination_id</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1480</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>DatabaseLookup</type>
    <connection>morocco_tourism</connection>
    <table>dim_destinations</table>
    <keys>
      <key>
        <condition>=</condition>
        <name>nom_destination</name>
        <field>destination</field>
      </key>
    </keys>
    <values>
      <value>
        <name>destinations_id</name>
        <rename>destinations_id</rename>
      </value>
    </values>
  </step>
  <step>
    <name>Inserer_fact_top_destinations</name>
    <type/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>1710</xloc>
      <yloc>100</yloc>
      <draw>Y</draw>
    </GUI>
    <type>InsertUpdate</type>
    <connection>morocco_tourism</connection>
    <table>fact_top_destinations</table>
    <commit>1000</commit>
    <use_batch>Y</use_batch>
    <lookup>
      <key>
        <name>destinations_id</name>
        <field>destinations_id</field>
        <condition>=</condition>
      </key>
    </lookup>
    <update>
      <update>
        <name>non_residents</name>
        <field>non_residents</field>
        <update>Y</update>
      </update>
      <update>
        <name>residents</name>
        <field>residents</field>
        <update>Y</update>
      </update>
      <update>
        <name>total_visiteurs</name>
        <field>total</field>
        <update>Y</update>
      </update>
      <update>
        <name>taux_occupation_pct</name>
        <field>taux_occupation_pct</field>
        <update>Y</update>
      </update>
    </update>
  </step>
  <hop>
    <from>Lire_top_destinations</from>
    <to>Standardiser_colonnes</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Standardiser_colonnes</from>
    <to>Nettoyer_numeriques</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Nettoyer_numeriques</from>
    <to>Convertir_types</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Convertir_types</from>
    <to>Trim_destination</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Trim_destination</from>
    <to>Inserer_dim_destinations</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Inserer_dim_destinations</from>
    <to>Lookup_destination_id</to>
    <enabled>Y</enabled>
  </hop>
  <hop>
    <from>Lookup_destination_id</from>
    <to>Inserer_fact_top_destinations</to>
    <enabled>Y</enabled>
  </hop>
</transformation>
